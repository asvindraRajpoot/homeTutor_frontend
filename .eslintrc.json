{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "typescript"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "typescript/no-unused-vars": "warn"
  }
}
